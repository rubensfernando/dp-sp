        $(document).ready(function() {            
            if ($(".exibeNota").val() == "0") {
                $(".fundo").hide();
                $(".pnlNota").hide();
            }
            $("#fechar_nota").click(function() {
                $(".fundo").hide();
                $(".pnlNota").hide();
                $(".exibeNota").val('0');
            });
        });
